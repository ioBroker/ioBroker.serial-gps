{
  "name": "iobroker.serial-gps",
  "version": "0.0.4",
  "description": "This adapter reads position from GPS mouse (serial or USB)",
  "author": {
    "name": "bluefox",
    "email": "dogafox@gmail.com"
  },
  "homepage": "https://github.com/ioBroker/ioBroker.serial-gps",
  "keywords": [
    "ioBroker",
    "gps"
  ],
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ioBroker/ioBroker.serial-gps"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.3.2",
    "serialport": "^13.0.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^5.0.0",
    "@alcalzone/release-script-plugin-iobroker": "^4.0.0",
    "@alcalzone/release-script-plugin-license": "^4.0.0",
    "@iobroker/build-tools": "^2.0.15",
    "@iobroker/eslint-config": "^2.2.0",
    "@iobroker/legacy-testing": "^2.0.2",
    "@iobroker/testing": "^5.2.2",
    "@iobroker/types": "^7.1.0",
    "@types/node": "^25.0.1",
    "typescript": "~5.9.3"
  },
  "bugs": {
    "url": "https://github.com/ioBroker/ioBroker.serial-gps/issues"
  },
  "main": "build/main.js",
  "files": [
    "admin/",
    "doc/",
    "img/",
    "keys/",
    "build/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "test:integration": "mocha test/adapter.test.js --exit",
    "test:package": "mocha test/package.test.js --exit",
    "test": "npm run test:integration",
    "lint": "eslint -c eslint.config.mjs",
    "build": "tsc -p tsconfig.build.json",
    "release": "release-script",
    "release-patch": "release-script patch --yes",
    "release-minor": "release-script minor --yes",
    "release-major": "release-script major --yes",
    "update-packages": "npx -y npm-check-updates"
  },
  "license": "MIT",
  "readmeFilename": "README.md"
}
